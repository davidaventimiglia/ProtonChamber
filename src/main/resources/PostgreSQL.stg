getEntityCollection_select(info) ::= <%
with t as (select <info.table>.* from <info.tables> where <info.keys; separator=" and "> <info.limit>) select * from t
%>

getEntityCollection_count_n(info) ::= <%
with t as (select count(1) from <info.tables> where <info.keys>) select * from t
%>

getEntityCollection_count_1(info) ::= <%
select 1
%>